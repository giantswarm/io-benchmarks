; Reads a 1G file in sequential order with different block sizes synchronously and O_DIRECT is turned on.
; This should bypass any Kernel buffering and test direct filesystem or device performance.
; Workloads like these usually appear in database transaction logs.
[global]
ioengine=sync
rw=read
direct=1
size=1G

[sync-write-4k]
bs=4K
stonewall

[sync-write-128k]
bs=128K
stonewall

[sync-write-256K]
bs=256K
stonewall

[sync-write-512k]
bs=512K
stonewall

[sync-write-1M]
bs=1M
stonewall
